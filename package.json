{
  "name": "guhd-eats-bot",
  "version": "1.0.0",
  "description": "GUHD EATS Discord bot with VP economy system",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "node src/index.js",
    "start": "node src/index.js",
    "test": "vitest run --config vitest.config.js",
    "test:watch": "vitest --config vitest.config.js",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config .eslintrc.cjs \"src/**/*.js\" \"tests/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"src/**/*.js\" \"tests/**/*.js\" README.md package.json",
    "format:fix": "prettier --write \"src/**/*.js\" \"tests/**/*.js\" README.md package.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "discord",
    "bot",
    "economy",
    "vouch"
  ],
  "author": "GUHD EATS",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "prisma": "^6.17.1",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
